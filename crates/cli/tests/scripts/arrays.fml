var a = [
	0,
	1,
	2,
	3,
];

assert(a[0] == 0);
assert(a[1] == 1);
assert(a[2] == 2);
assert(a[3] == 3);

a[0] = 10;
a[1] = 11;
a[2] = 12;
a[3] = 13;

assert(a[0] == 10);
assert(a[1] == 11);
assert(a[2] == 12);
assert(a[3] == 13);

var b = array_create(3, "hello");
assert(b[0] == "hello");
assert(b[1] == "hello");
assert(b[2] == "hello");
assert(b[3] == undefined);

var c = array_create_ext(3, function(i) {
	return i;
});
assert(c[0] == 0);
assert(c[1] == 1);
assert(c[2] == 2);
assert(c[3] == undefined);

return true;
