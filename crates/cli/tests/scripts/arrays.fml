var a = [
	0,
	1,
	2,
	3,
];

assert(a[0] == 0);
assert(a[1] == 1);
assert(a[2] == 2);
assert(a[3] == 3);

a[0] = 10;
a[1] = 11;
a[2] = 12;
a[3] = 13;

assert(a[0] == 10);
assert(a[1] == 11);
assert(a[2] == 12);
assert(a[3] == 13);

var b = array_create(3, "hello");
assert(b[0] == "hello");
assert(b[1] == "hello");
assert(b[2] == "hello");
assert(b[3] == undefined);

var c = array_create_ext(3, function(i) {
	return i;
});
assert(c[0] == 0);
assert(c[1] == 1);
assert(c[2] == 2);
assert(c[3] == undefined);

var c = [
	0,
	1,
	2,
	3,
	4,
	5,
	6,
];

array_delete(c, -2, 2);
assert(array_length(c) == 5);
assert(c[0] == 0);
assert(c[4] == 4);
array_delete(c, 3, -2);
assert(array_length(c) == 3);
assert(c[0] == 0);
assert(c[1] == 1);
assert(c[2] == 4);

var c = [
	0,
	1,
	2,
	0,
	1,
	2,
];


assert(array_get_index(c, 1, 3) == 4);
assert(array_get_index(c, 1, 3, -4) == 1);
assert(array_get_index(c, 1, 5, -3) == 4);

return true;
